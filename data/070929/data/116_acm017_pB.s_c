(require (lib "48.ss" "srfi"))

(define doit (lambda (s)
  (if (eq? s '()) 0
    (if (number? s) s
      (+ (doit (cadr s)) (* (+ (car s) (car s) -1) (doit (caddr s))))))))

(define main (lambda ()
  (let ((x (read)))
    (if (eq? x '()) #f
      (begin
	(format (current-output-port) "~0,2F" (doit x)) (newline)
	(main))))))

(main)
