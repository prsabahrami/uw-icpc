(require (lib "etc.ss"))
(require (lib "list.ss"))
(require (lib "math.ss"))

(define (read-list n)
  (if (= 0 n)
      '()
      (cons (read) (read-list (- n 1)))))
(define (cost knights heads)
  (cond ((empty? heads) 0)
        ((empty? knights) "Loowater is doomed!")
        ((> (car heads) (car knights)) (cost (cdr knights) heads))
        (#t (let ((rest (cost (cdr knights) (cdr heads))))
              (if (number? rest)
                  (+ (car knights) rest)
                  rest)))))
(define (go)
  (let ((n (read))
        (m (read)))
    (if (= 0 n)
        '()
         (let ((heads (sort (read-list n) <))
               (knights (sort (read-list m) <)))
           (begin
             (display (cost knights heads))
             (newline)
             (go)
           )))))
(go)

